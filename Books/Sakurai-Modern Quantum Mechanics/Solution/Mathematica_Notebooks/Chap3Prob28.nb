Cell[BoxData[
 RowBox[{
  RowBox[{"Cos", "[", "\[Alpha]", "]"}], "+", 
  FractionBox[
   RowBox[{
    SqrtBox[
     RowBox[{
      RowBox[{"-", 
       SuperscriptBox["\[Alpha]", "2"]}], "+", 
      SuperscriptBox["\[Beta]", "2"]}]], " ", 
    RowBox[{"Sin", "[", "\[Alpha]", "]"}]}], "\[Alpha]"]}]], "Output",
 CellChangeTimes->{3.8027937475549192`*^9},
