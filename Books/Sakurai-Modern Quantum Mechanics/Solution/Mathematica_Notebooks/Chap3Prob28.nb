Cell["\<\
Now we want to do (b), so we need to match the boundary conditions at r=a.\
\>", "Text",
 CellChangeTimes->{{3.731160933124298*^9, 3.731160937770404*^9}, {
  3.731160974705027*^9, 
  3.7311610257747173`*^9}},ExpressionUUID->"82d2ba26-c26f-4593-8403-\
018c802b8505"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"Rin", "=", 
   RowBox[{"A", " ", 
    RowBox[{
     RowBox[{"Sin", "[", 
      RowBox[{"\[Alpha]", " ", 
       RowBox[{"r", "/", "a"}]}], "]"}], "/", 
     RowBox[{"(", 
      RowBox[{"\[Alpha]", " ", 
       RowBox[{"r", "/", "a"}]}], ")"}]}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Rex", "=", 
   RowBox[{"B", " ", 
    RowBox[{"Exp", "[", 
     RowBox[{
      RowBox[{"-", "\[Gamma]"}], " ", 
      RowBox[{"r", "/", "a"}]}], "]"}], 
    RowBox[{"(", 
     RowBox[{"a", "/", "r"}], ")"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"eq1", "=", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{"Rin", "/.", 
     RowBox[{"r", "\[Rule]", "a"}]}], ")"}], "\[Equal]", 
   RowBox[{"(", 
    RowBox[{"Rex", "/.", 
     RowBox[{"r", "\[Rule]", "a"}]}], ")"}]}]}], "\[IndentingNewLine]", 
 RowBox[{"eq2", "=", 
  RowBox[{"Simplify", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"D", "[", 
        RowBox[{"Rin", ",", "r"}], "]"}], "/.", 
       RowBox[{"r", "\[Rule]", "a"}]}], ")"}], "\[Equal]", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"D", "[", 
        RowBox[{"Rex", ",", "r"}], "]"}], "/.", 
       RowBox[{"r", "\[Rule]", "a"}]}], ")"}]}], ",", 
    RowBox[{"a", ">", "0"}]}], "]"}]}]}], "Input",
 CellChangeTimes->{{3.731432745288356*^9, 3.731432853596492*^9}, {
  3.7314328871342297`*^9, 3.7314329504413958`*^9}, {3.731433047296092*^9, 
  3.731433073588131*^9}, {3.731433395338263*^9, 3.731433453908627*^9}},
 CellLabel->"In[8]:=",ExpressionUUID->"fb9cc28a-0c29-4050-85d4-a05c224e7609"],

Cell[BoxData[
 RowBox[{
  FractionBox[
   RowBox[{"A", " ", 
    RowBox[{"Sin", "[", "\[Alpha]", "]"}]}], "\[Alpha]"], "\[Equal]", 
  RowBox[{"B", " ", 
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{"-", "\[Gamma]"}]]}]}]], "Output",
 CellChangeTimes->{3.802793747353725*^9},
 CellLabel->"Out[10]=",ExpressionUUID->"4a3d4f0c-8bde-4371-87f8-213e2315dd06"],

Cell[BoxData[
 RowBox[{
  RowBox[{"B", "+", 
   RowBox[{"B", " ", "\[Gamma]"}], "+", 
   RowBox[{"A", " ", 
    SuperscriptBox["\[ExponentialE]", "\[Gamma]"], " ", 
    RowBox[{"Cos", "[", "\[Alpha]", "]"}]}]}], "\[Equal]", 
  FractionBox[
   RowBox[{"A", " ", 
    SuperscriptBox["\[ExponentialE]", "\[Gamma]"], " ", 
    RowBox[{"Sin", "[", "\[Alpha]", "]"}]}], "\[Alpha]"]}]], "Output",
 CellChangeTimes->{3.802793747476672*^9},
 CellLabel->"Out[11]=",ExpressionUUID->"e1629397-1daf-43b4-adac-e05a2052e3dc"]
}, Open  ]],

Cell["\<\
Get the array of coefficients and get the determinant (with substitution for \
\[Gamma])\
\>", "Text",
 CellChangeTimes->{{3.731160933124298*^9, 3.731160937770404*^9}, {
  3.731160974705027*^9, 3.7311610257747173`*^9}, {3.73151671610155*^9, 
  3.731516745107778*^9}},ExpressionUUID->"c7318997-60ff-43c7-88e8-\
cc9a6a4d763d"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"mat", "=", 
  RowBox[{
   RowBox[{"Normal", "[", 
    RowBox[{"CoefficientArrays", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"eq1", ",", "eq2"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"A", ",", "B"}], "}"}]}], "]"}], "]"}], "[", 
   RowBox[{"[", "2", "]"}], "]"}]}]], "Input",
 CellChangeTimes->{{3.7315165186856337`*^9, 3.7315165398761473`*^9}, {
  3.731516576684001*^9, 3.731516596281494*^9}},
