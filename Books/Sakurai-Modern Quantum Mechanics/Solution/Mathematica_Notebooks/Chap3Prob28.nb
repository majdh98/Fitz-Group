Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     FractionBox[
      RowBox[{"Sin", "[", "\[Alpha]", "]"}], "\[Alpha]"], ",", 
     RowBox[{"-", 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"-", "\[Gamma]"}]]}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       SuperscriptBox["\[ExponentialE]", "\[Gamma]"], " ", 
       RowBox[{"Cos", "[", "\[Alpha]", "]"}]}], "-", 
      FractionBox[
       RowBox[{
        SuperscriptBox["\[ExponentialE]", "\[Gamma]"], " ", 
        RowBox[{"Sin", "[", "\[Alpha]", "]"}]}], "\[Alpha]"]}], ",", 
     RowBox[{"1", "+", "\[Gamma]"}]}], "}"}]}], "}"}]], "Output",
 CellChangeTimes->{3.802793747515682*^9},
 CellLabel->"Out[12]=",ExpressionUUID->"585e9384-d179-4eac-a1f0-6dc7d7300ded"]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"det", "=", 
  RowBox[{
   RowBox[{"Det", "[", "mat", "]"}], "/.", 
   RowBox[{"\[Gamma]", "\[Rule]", 
    RowBox[{"Sqrt", "[", 
     RowBox[{
      RowBox[{"\[Beta]", "^", "2"}], "-", 
      RowBox[{"\[Alpha]", "^", "2"}]}], "]"}]}]}]}]], "Input",
